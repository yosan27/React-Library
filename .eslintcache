form-group row\">\n                                <label for=\"nominal\" className=\"col-sm-3\">\n                                  Year\n                      </label>\n                                <input\n                                  name=\"year\"\n                                  className=\"col-sm-6\"\n                                  id=\"year\"\n                                  placeholder=\"2021\"\n                                  autoComplete=\"off\"\n                                  value={this.state.year}\n                                  onChange={(e) => this.handleChange(e, e.target.value)}\n                                ></input>\n                              </div>\n\n                              <div className=\"form-group row\">\n                                <label for=\"validTo\" className=\"col-sm-3\">\n                                  Author\n                      </label>\n                                <input\n                                  name=\"author\"\n                                  className=\"col-sm-6\"\n                                  placeholder=\"Pramoedya A. TOer\"\n                                  id=\"author\"\n                                  autoComplete=\"off\"\n                                  value={this.state.author}\n                                  onChange={(e) => this.handleChange(e, e.target.value)}\n                                // onInput={this.maxLengthCheck}\n                                ></input>\n                              </div>\n\n                              <div className=\"form-group row\">\n                                <label for=\"validTo\" className=\"col-sm-3\">\n                                  Category\n                      </label>\n                                <input\n                                  name=\"category\"\n                                  className=\"col-sm-6\"\n                                  placeholder=\"History\"\n                