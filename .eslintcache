ame=\"card-header\">\r\n                      <h3 className=\"card-title\">Donation Management</h3>\r\n                    </div>\r\n                    <div className=\"card-body\">\r\n                      <Table\r\n                        responsive\r\n                        striped\r\n                        id=\"donation-list\"\r\n                        style={{ width: \"100%\" }}\r\n                      >\r\n                        <thead>\r\n                          <tr>\r\n                            <th>Book ID</th>\r\n                            <th>Action</th>\r\n                            <th>Book Title</th>\r\n                            <th>Author</th>\r\n                            <th>Categories</th>\r\n                            <th>Status</th>\r\n                          </tr>\r\n                        </thead>\r\n                        <tbody>\r\n                          {this.state.donationList.map((donation, index) => {\r\n                            return (\r\n                              <tr>\r\n                                <td>{donation.id}</td>\r\n                                <td>\r\n                                  <span className=\"d-flex justify-content-center\">\r\n                                    <button\r\n                                      className=\"btn btn-warning\"\r\n                                      data-toggle=\"modal\"\r\n                                      data-target=\"#edit\"\r\n                                      onClick={() => this.getDetail(donation.id)}\r\n                                    >\r\n                                      <i className=\"fa fa-eye\"></i>\r\n                                    </button>\r\n                                    <button\r\n                                      className=\"btn btn-success\"\r\n                                      data-toggle=\"modal\"\r\n                                      data-target=\"#delete\"\r\n                              