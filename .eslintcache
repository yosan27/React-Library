0\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookDescription\"\n                                  class=\"form-control\" \n                                  id=\"addBookDescription\" \n                                  placeholder=\"Description...\" \n                                  onChange={(e) => this.setState({description : e.target.value})}\n                                  value={description} \n                                  data-attribute-name=\"Description\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addCover\" class=\"col-sm-2 col-form-label\">Cover</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookCover\"\n                                  class=\"form-control\" \n                                  id=\"addBookCover\" \n                                  placeholder=\"Cover...\" \n                                  onChange={(e) => this.setState({cover : e.target.value})}\n                                  value={cover} \n                                  data-attribute-name=\"Cover\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookPages\" class=\"col-sm-2 col-form-label\">Number of pages</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookPages\"\n                                  class=\"form-control\" \n                                  id=\"addBookPages\" \n                                  placeholder=\"Pages...\" \n                                  onChange={(e) => this.setState({numberOfPages : e.target.value})}\n                                  value={numberOfPages} \n                                  data-attribute-name=\"Pages\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookLanguage\" class=\"col-sm-2 col-form-label\">Language</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"language\"\n                                  class=\"form-control\" \n                                  id=\"addLanguage\" \n                                  placeholder=\"Language...\" \n                                  onChange={(e) => this.setState({language : e.target.value})}\n                                  value={language} \n                                  data-attribute-name=\"Language\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                            </form>\n                          </div>\n                        </div>\n                      </Modal.Body>\n                      <Modal.Footer>\n                        <Button className=\"btn btn-secondary\" variant=\"secondary\" onClick={this.handleCloseModal}>\n                          <i class=\"fa fa-times-circle\"></i> Close\n                        </Button>\n                        <Button id=\"buttonAddBook\" disabled={disableSubmitting} type=\"submit\" className=\"btn btn-success\" variant=\"primary\" onClick={this.handleAddBookDetail}>\n                          <i class=\"fa fa-plus\"></i> Add\n                        </Button>\n                      </Modal.Footer>\n                    </Modal>\n                    {/* modal add */}\n\n                    {/* modal edit */}\n                    <Modal show={showEdit} onHide={this.handleCloseModal}>\n                      <Modal.Header closeButton>\n                        <Modal.Title>Edit Data</Modal.Title>\n                      </Modal.Header>\n                      <Modal.Body>\n                        <div class='container'>\n                          <div class=\"modal-body\">\n                          <form>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookTitle\" class=\"col-sm-2 col-form-label\">Book Title</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookTitle\"\n                                  class=\"form-control\" \n                                  id=\"addBookTitle\" \n                                  placeholder=\"Title...\" \n                                  onChange={(e) => this.setState({bookTitle : e.target.value})}\n                                  value={bookTitle} \n                                  data-attribute-name=\"Title\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookSubtitle\" class=\"col-sm-2 col-form-label\">Book Subtitle</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookSubtitle\"\n                                  class=\"form-control\" \n                                  id=\"addBookSubtitle\" \n                                  placeholder=\"Subtitle...\" \n                                  onChange={(e) => this.setState({bookSubtitle : e.target.value})}\n                                  value={bookSubtitle} \n                                  data-attribute-name=\"Subtitle\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addDescription\" class=\"col-sm-2 col-form-label\">Description</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookDescription\"\n                                  class=\"form-control\" \n                                  id=\"addBookDescription\" \n                                  placeholder=\"Description...\" \n                                  onChange={(e) => this.setState({description : e.target.value})}\n                                  value={description} \n                                  data-attribute-name=\"Description\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addCover\" class=\"col-sm-2 col-form-label\">Cover</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookCover\"\n                                  class=\"form-control\" \n                                  id=\"addBookCover\" \n                                  placeholder=\"Cover...\" \n                                  onChange={(e) => this.setState({cover : e.target.value})}\n                                  value={cover} \n                                  data-attribute-name=\"Cover\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookPages\" class=\"col-sm-2 col-form-label\">Number of pages</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"bookPages\"\n                                  class=\"form-control\" \n                                  id=\"addBookPages\" \n                                  placeholder=\"Pages...\" \n                                  onChange={(e) => this.setState({numberOfPages : e.target.value})}\n                                  value={numberOfPages} \n                                  data-attribute-name=\"Pages\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                              <div class=\"form-group row\">\n                                <label for=\"addBookLanguage\" class=\"col-sm-2 col-form-label\">Language</label>\n                                <div class=\"col-sm-10\">\n                                  <input \n                                  type=\"text\" \n                                  name=\"language\"\n                                  class=\"form-control\" \n                                  id=\"addLanguage\" \n                                  placeholder=\"Language...\" \n                                  onChange={(e) => this.setState({language : e.target.value})}\n                                  value={language} \n                                  data-attribute-name=\"Language\"\n                                  data-async\n                                  />\n                                </div>\n                              </div>\n                            </form>\n                          </div>\n                        </div>\n                      </Modal.Body>\n                      <Modal.Footer>\n                        <Button className=\"btn btn-secondary\" variant=\"secondary\" onClick={this.handleCloseModal}>\n                          Cancel\n                        </Button>\n                        <Button className=\"btn btn-success\" variant=\"primary\" onClick={this.handleSaveEdit}>\n                          Save\n                        </Button>\n                      </Modal.Footer>\n                    </Modal>\n                    {/* modal edit */}\n\n                    {/* modal delete */}\n                    <Modal show={showDelete} onHide={this.handleCloseModal}>\n                      <Modal.Header closeButton>\n                        <Modal.Title>Delete Data</Modal.Title>\n                      </Modal.Header>\n                      <Modal.Body>\n                        <div class='container'>\n                          <p>Are you sure you want to delete publisher data?</p>\n                        </div>\n                      </Modal.Body>\n                      <Modal.Footer>\n                        <Button className=\"btn btn-secondary\" variant=\"secondary\" onClick={this.handleCloseModal}>\n                          Close\n                        </Button>\n                        <Button className=\"btn btn-warning\" variant=\"primary\" onClick={this.handleDelete}>\n                          Delete\n               